{
  "name": "zaengle/audits",
  "description": "A package to manage model audits",
  "license": "MIT",
  "keywords": ["laravel", "audit"],
  "authors": [
    {
      "name": "Zaengle",
      "email": "info@zaengle.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "8.*|9.*|10.*",
    "illuminate/console": "8.*|9.*|10.*",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": ">=0.9.9",
    "phpunit/phpunit": ">=4.1",
    "orchestra/testbench": "~8.0"
  },
  "autoload": {
    "psr-4": {
      "Zaengle\\Audit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zaengle\\Audit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zaengle\\Audit\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
